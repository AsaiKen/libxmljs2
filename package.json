{
  "name": "libxmljs2",
  "author": "marudor",
  "contributors": [
    "Jeff Smick",
    "Marco Rogers"
  ],
  "binary": {
    "module_name": "xmljs",
    "module_path": "./build/Release/",
    "host": "https://github.com",
    "remote_path": "./libxmljs/libxmljs/releases/download/v{version}/",
    "package_name": "{node_abi}-{platform}-{arch}-{libc}.tar.gz"
  },
  "description": "libxml bindings for v8 javascript engine",
  "version": "0.20.0",
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build --loglevel http",
    "test": "node --expose_gc ./node_modules/.bin/nodeunit test"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/marudor/libxmljs.git"
  },
  "bugs": {
    "url": "http://github.com/marudor/libxmljs/issues"
  },
  "main": "./index",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "bindings": "~1.3.0",
    "nan": "~2.13.1",
    "node-pre-gyp": "~0.12.0"
  },
  "devDependencies": {
    "nodeunit": "~0.11.2",
    "semver": "~5.5.0"
  }
}
